    <servlet-name>getModel</servlet-name>
    <url-pattern>/getModel</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>getLogList</servlet-name>
    <servlet-class>jiuzhou.app.web.GetLogServlet</servlet-class>
  </servlet>
